group test_csv2stg;

// template group file for use by StringTemplate component in STELLAR project

HEADER() ::= <<
<html>
<head>
<title>This is the header</title>
</head>
<body>
>>

FOOTER() ::= <<
<p>This is the footer</p>
</body>
</html>
>>

RECORD(item1, item2, item3, item4) ::= <<
<ol>
	<li>$item1$</li>
	<li>$item2$</li>
	<li>$item3$</li>
	<li>$item4$</li>
</ol>
 >>


