group test_csv2stg_skos;

// template group file for use by StringTemplate component in STELLAR project
PREFIX() ::= "http://tempuri/stellar/base/"

HEADER() ::= <<
<rdf:RDF 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:skos="http://www.w3.org/2004/02/skos/core#" 
	xmlns:ecrm="http://erlangen-crm.org/101001/" 
	xmlns:crmeh="http://purl.org/crmeh#" 
	xmlns:dc="http://purl.org/dc/elements/1.1/" 
	xmlns:dcterms="http://purl.org/dc/terms/">	
>>

FOOTER() ::= <<
</rdf:RDF>
>>

MAIN(data) ::= <<
$HEADER()$
$data:{ d | 
<skos:Concept rdf:about="$PREFIX()$$d.id$">
	<skos:prefLabel rdf:resource="$PREFIX()$$d.item2$"/>
	<skos:inScheme rdf:resource="$d.item3$"/>
</skos:Concept>
}$ 
$FOOTER()$
 >>